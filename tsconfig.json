{
  "compilerOptions": {
    "declaration": false,
    "module": "commonjs",
    "moduleResolution": "node",
    "experimentalDecorators": true,
    "emitDecoratorMetadata": true,
    "target": "es2017",
    "noImplicitAny": true,
    "outDir": "lib",
    "preserveConstEnums": true,
    "removeComments": true,
    "noUnusedLocals": true,
    "lib": [
      "es6",
      "esnext"
    ]
  },
}
